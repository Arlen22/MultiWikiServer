{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */
    "types": [
      "node",
    ],
    "lib": [
      "es2020"
    ],
    "target": "es2020",
    "module": "preserve",
    "moduleResolution": "bundler",
    "noEmit": true,
    "resolveJsonModule": true,
    // "emitDeclarationOnly": true,
    // "declaration": true,
    "outDir": "../../react-user-mgmt/server",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true,
    "forceConsistentCasingInFileNames": true,
    "checkJs": true,
    "allowJs": true,
    "paths": {
      "tiddlywiki": [
        "./tiddlywiki-types"
      ]
    }
  },
  "files": [
    // "src/**/*.ts",
    // "src/**/*.d.ts",
    // "./mws.*.ts",
    "./src/index.ts",
    "./src/global.ts",
  ],
  "exclude": [
    "src/db-node-sqlite3-wasm/api.js",
  ]
}