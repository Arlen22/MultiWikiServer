{
  "name": "@tiddlywiki/server",
  "description": "TiddlyWiki Web Server",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "tsup": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./src/*": "./src/*"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "docs": "node scripts.mjs docs",
    "start": "node scripts.mjs start",
    "certs": "(cd runtime-config && bash localhost_certs.sh)",
    "tsc": "tsc -p tsconfig.json --skipLibCheck",
    "tsc:all": "npm run tsc:src; npm run tsc:admin; npm run tsc:client;",
    "tsc:admin": "cd react-user-mgmt && npm run tsc",
    "tsc:client": "cd plugins/client && npm run tsc || true",
    "build": "tsup",
    "install-android": "GYP_DEFINES=\"android_ndk_path=''\" npm install",
    "env": "env",
    "test:pack": "(git clean -dfx tests && npm pack --pack-destination tests && cd tests && npm install && npm install ./tiddlywiki-mws-$npm_package_version.tgz --no-save && npm test)",
    "test": "(git clean -dfx tests && cd tests && npm install .. --no-save && npm test)",
    "fulltest": "mv node_modules node_modules_old; npm run test:pack; mv node_modules_old node_modules",
    "prepublishOnly": "npm run build",
    "prisma:migrate": "DATABASE_URL=file:test.sqlite npx prisma migrate dev"
  },
  "dependencies": {
    "@types/bytes": "^3.1.5",
    "@types/compression": "^1.8.0",
    "@types/debug": "^4.1.12",
    "@types/mime-types": "^2.1.4",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^22.13.1",
    "@types/send": "^0.17.4",
    "bytes": "^3.1.2",
    "chalk": "^5.4.1",
    "commander": "13.1.0",
    "compressible": "^2.0.18",
    "compression": "^1.8.0",
    "debug": "^4.4.1",
    "esbuild": "0.25.2",
    "helmet": "^8.1.0",
    "mime-types": "^2.1.35",
    "negotiator": "^1.0.0",
    "send": "^1.2.0",
    "source-map-support": "^0.5.21",
    "supports-color": "^10.0.0",
    "try": "^1.0.0-beta.5",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "uuid": "^11.1.0",
    "vary": "^1.1.2",
    "zod": "^3.25.49",
    "zod-to-ts": "^1.2.0",
    "zod-validation-error": "^3.4.1"
  },
  "devDependencies": {}
}